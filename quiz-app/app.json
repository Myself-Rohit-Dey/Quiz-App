{
  "expo": {
    "name": "quiz-app",
    "slug": "quiz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/quiz-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/quiz-splash-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/quiz-adaptive-icon-lg.png"
      },
      "package": "com.myselfrohitdey.quizapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "02d1e3b2-ab66-4c1d-aa2f-98324f3486d7"
      }
    }
  }
}
